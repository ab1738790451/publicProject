<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woshen.acl.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.woshen.acl.entity.Menu">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="url" property="url" />
        <result column="parent_id" property="parentId" />
        <result column="priority" property="priority" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="app_id" property="appId" />
        <result column="is_menu" property="isMenu" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, url, parent_id, priority, status, create_time, update_time, app_id, is_menu
    </sql>

</mapper>
